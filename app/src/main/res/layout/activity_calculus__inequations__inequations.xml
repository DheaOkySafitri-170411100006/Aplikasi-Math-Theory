<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Calculus_Inequations_Inequations">
    <TextView
        android:id="@+id/textViewpenjelasan"
        android:layout_width="match_parent"
        android:layout_height="125dp"
        android:background="@drawable/shape"
        android:padding="12dp"
        android:layout_marginTop="18dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:text="Learn how to solvedefinite integrals using Barrow's rule, integrals of piecewise functions and improper integrals."/>
    <ListView
        android:id="@+id/listView"
        android:layout_width="match_parent"
        android:layout_height="460dp"></ListView>

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/bottom_navigation"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#FFFFFF"
        app:itemBackground="@color/colorAccent"
        app:itemIconTint="@drawable/selector"
        app:itemTextColor="@drawable/selector"
        app:menu="@menu/menu_navigation" />

</androidx.constraintlayout.widget.ConstraintLayout>